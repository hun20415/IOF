<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
<class name="kr.ac.iof.model.UserGroup" table="SYS_USER_GROUP">
        <id name="usergroupID" type="int">
            <column name="USERGROUP_ID" precision="38" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="usergroupName" type="string">
            <column name="USERGROUP_NAME" length="20" not-null="false" />
        </property>
           </class>
</hibernate-mapping>